{"ast":null,"code":"import PesonalData from \"./user/PesonalData.vue\";\nimport WorkersUser from \"./user/WorkersUser.vue\";\nimport TicketsUser from \"./user/TicketsUser.vue\";\nimport MinersUser from \"./user/MinersUser.vue\";\nexport default {\n  name: \"AppUser\",\n  components: {\n    PesonalData,\n    WorkersUser,\n    TicketsUser,\n    MinersUser\n  },\n  data() {\n    return {\n      id: 25616,\n      email: \"zankov.vadik@yandex.ru\",\n      active: 4\n    };\n  },\n  methods: {\n    changeStat(n) {\n      try {\n        this.active = n;\n      } catch (err) {\n        console.log(err);\n      }\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["PesonalData","WorkersUser","TicketsUser","MinersUser","name","components","data","id","email","active","methods","changeStat","n","err","console","log","mounted"],"sources":["/Users/vadim/Desktop/hac-25-admin/src/pages/AppUser.vue"],"sourcesContent":["<script>\nimport PesonalData from \"./user/PesonalData.vue\";\nimport WorkersUser from \"./user/WorkersUser.vue\";\nimport TicketsUser from \"./user/TicketsUser.vue\";\nimport MinersUser from \"./user/MinersUser.vue\";\nexport default {\n  name: \"AppUser\",\n  components: { PesonalData, WorkersUser, TicketsUser, MinersUser },\n  data() {\n    return {\n      id: 25616,\n      email: \"zankov.vadik@yandex.ru\",\n      active: 4,\n    };\n  },\n  methods: {\n    changeStat(n) {\n      try {\n        this.active = n;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n  },\n  mounted() {},\n};\n</script>\n<template>\n  <main class=\"wrapper\">\n    <div class=\"group-title\">\n      <h1>\n        Пользователь <span>ID {{ id }} ({{ email }})</span>\n      </h1>\n      <button class=\"btn edit\">Редактировать</button>\n    </div>\n    <div class=\"wrap-btns\">\n      <button\n        class=\"btn\"\n        @click=\"changeStat(1)\"\n        :class=\"{ active: active == 1 }\"\n      >\n        Личные данные\n      </button>\n      <button\n        class=\"btn\"\n        @click=\"changeStat(2)\"\n        :class=\"{ active: active == 2 }\"\n      >\n        Воркеры\n      </button>\n      <button\n        class=\"btn\"\n        @click=\"changeStat(3)\"\n        :class=\"{ active: active == 3 }\"\n      >\n        Асики клиента\n      </button>\n      <button\n        class=\"btn\"\n        @click=\"changeStat(4)\"\n        :class=\"{ active: active == 4 }\"\n      >\n        Тикеты клиента\n      </button>\n    </div>\n    <PesonalData v-if=\"active == 1\" />\n    <WorkersUser v-if=\"active == 2\" />\n    <MinersUser v-if=\"active == 3\" />\n    <TicketsUser v-if=\"active == 4\" />\n  </main>\n</template>\n<style scoped>\n.wrapper {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n  padding: 20px;\n}\n\nh1 span {\n  color: #195ee6;\n  text-decoration: underline;\n  font-weight: 600;\n  font-size: 24px;\n  line-height: 32.78px;\n}\n.wrap-btns {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.btn {\n  padding: 14.5px 24px;\n  border-radius: 8px;\n  color: #5b6171;\n  font-weight: 600;\n  transition: all 500ms ease;\n}\n.edit {\n  color: #195ee6;\n}\n\n.active {\n  background-color: rgba(25, 94, 230, 0.1);\n  color: #195ee6;\n}\n\n.group-title {\n  width: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n</style>\n"],"mappings":"AACA,OAAOA,WAAU,MAAO,wBAAwB;AAChD,OAAOC,WAAU,MAAO,wBAAwB;AAChD,OAAOC,WAAU,MAAO,wBAAwB;AAChD,OAAOC,UAAS,MAAO,uBAAuB;AAC9C,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IAAEL,WAAW;IAAEC,WAAW;IAAEC,WAAW;IAAEC;EAAW,CAAC;EACjEG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,EAAE,EAAE,KAAK;MACTC,KAAK,EAAE,wBAAwB;MAC/BC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACC,CAAC,EAAE;MACZ,IAAI;QACF,IAAI,CAACH,MAAK,GAAIG,CAAC;MACjB,EAAE,OAAOC,GAAG,EAAE;QACZC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB;IACF;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG,CAAC;AACb,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}